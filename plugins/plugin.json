{
    "$schema": "https://go.kicad.org/api/schemas/v1",
    "identifier": "com.github.mmuman.kicad-application-notes",
    "name": "Datasheet Application Notes",
    "description": "Calculate and apply application notes",
    "runtime": {
        "type": "python",
        "min_version": "3.9"
    },
    "actions": [
        {
            "identifier": "app-notes",
            "name": "App Notes",
            "description": "Apply datasheet application notes",
            "show-button": true,
            "scopes": [
                "pcb",
                "schematic"
            ],
            "entrypoint": "app_notes.py",
            "icons-light": [
                "icon.png"
            ],
            "icons-dark": [
                "icon.png"
            ]
        }
    ]
}
